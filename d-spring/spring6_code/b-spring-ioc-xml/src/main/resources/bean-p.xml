<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--p 命名空间注入-->
    <!--修改约束信息 xmlns:p="http://www.springframework.org/schema/p"-->
    <bean id="account" class="com.jiehfut.iocxml.p.Account"
    p:id="001" p:username="zhangsan" p:password="123456">
    </bean>

    <!--
        对于引用类型需要加 "-ref" 引入
        <bean id="studentSix" class="com.atguigu.spring6.bean.Student"
        p:id="1006" p:name="小明" p:clazz-ref="clazzOne" p:teacherMap-ref="teacherMap"></bean>
    -->

</beans>